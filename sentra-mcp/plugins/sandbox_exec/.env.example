# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-600000
PLUGIN_TIMEOUT_MS=300000

# sentra-config-ui 服务地址
# type: string
SANDBOX_EXEC_CONFIG_UI_BASE_URL=http://127.0.0.1:7245

# sentra-config-ui 安全 token（必须）
# type: string
SANDBOX_EXEC_SECURITY_TOKEN=REPLACE_ME

# 默认终端类型
# type: enum
# options: powershell | cmd | bash | zsh | sh
SANDBOX_EXEC_TERMINAL_TYPE=powershell
