# NapCat WebSocket 地址（正向模式：适配器连接 NapCat 的地址，OneBot 11 / NapCat OneBot v11）
# NapCat WebSocket 地址（正向连接）
# type: string
NAPCAT_WS_URL=ws://127.0.0.1:6700

# 连接模式（forward=正向，reverse=反向）
# 连接模式
# type: enum
# options: forward | reverse
NAPCAT_MODE=reverse

# NapCat/OneBot 实现使用的访问令牌（可选）
# 访问令牌
# type: string
NAPCAT_ACCESS_TOKEN=

# 日志级别（info | debug | warn | error | silent）
# 日志级别
# type: enum
# options: info | debug | warn | error | silent
LOG_LEVEL=debug

# 事件摘要打印策略（always | debug | never）
# 事件摘要策略
# type: enum
# options: always | debug | never
EVENT_SUMMARY=always

# 是否使用 JSON 结构化日志（true | false）
# 结构化日志
# type: boolean
# options: true | false
JSON_LOG=false

# 日志中消息文本最大长度（0=不限制，默认80）
# 文本最大长度
# type: number
# range: 0-1000
MESSAGE_TEXT_MAX_LENGTH=80

# 调用时是否自动等待 WS 连接就绪
# 自动等待连接
# type: boolean
# options: true | false
AUTO_WAIT_OPEN=true

# 命令前缀（例如 !help）
# 命令前缀
# type: string
COMMAND_PREFIX=!

# 是否启用自动重连 NapCat
# 自动重连
# type: boolean
# options: true | false
RECONNECT=true

# 最小重连间隔（毫秒）
# 最小重连间隔
# type: number
# range: 100-60000
RECONNECT_MIN_MS=1000

# 最大重连间隔（毫秒）
# 最大重连间隔
# type: number
# range: 1000-600000
RECONNECT_MAX_MS=15000

# 请求超时时间（毫秒）
# 请求超时
# type: number
# range: 1000-600000
REQUEST_TIMEOUT_MS=120000

# 最大并发请求数
# 最大并发
# type: number
# range: 1-100
RATE_MAX_CONCURRENCY=5

# 相邻请求的最小间隔时间（毫秒）
# 最小请求间隔
# type: number
# range: 0-60000
RATE_MIN_INTERVAL_MS=200

# 调用失败时的最大重试次数
# 最大重试次数
# type: number
# range: 0-10
RETRY_MAX_ATTEMPTS=3

# 第一次重试的延迟时间（毫秒）
# 首次重试延迟
# type: number
# range: 0-60000
RETRY_INITIAL_DELAY_MS=500

# 重试退避系数（指数退避）
# 重试退避系数
# type: number
# range: 1-10
RETRY_BACKOFF_FACTOR=2

# 重试抖动范围（毫秒）
# 重试抖动
# type: number
# range: 0-60000
RETRY_JITTER_MS=200

# 是否对消息事件做去重
# 事件去重
# type: boolean
# options: true | false
DEDUP_EVENTS=true

# 事件去重缓存过期时间（毫秒）
# 去重缓存 TTL
# type: number
# range: 1000-600000
DEDUP_TTL_MS=120000

# 群白名单（逗号分隔，空表示所有群均允许）
# 群白名单
# type: string
WHITELIST_GROUPS=

# 用户白名单（逗号分隔，空表示所有用户均允许）
# 用户白名单
# type: string
WHITELIST_USERS=

# 对非白名单消息是否记录日志（true=记录并忽略，false=静默丢弃）
# 记录过滤日志
# type: boolean
# options: true | false
LOG_FILTERED=false

# 反向 WebSocket 监听地址（反向模式：NapCat 连接到适配器）
# 反向 WebSocket 地址（包含端口与路径）
# type: string
REVERSE_WS_URL=ws://127.0.0.1:5140/onebot

# 是否启用消息流服务（WebSocket 实时消息推送）
# 启用消息流
# type: boolean
# options: true | false
ENABLE_STREAM=true

# 消息流服务监听端口
# 消息流端口
# type: number
# range: 1-65535
STREAM_PORT=6702

# 是否在推送中包含原始事件数据（调试用，会增加数据量）
# 包含原始事件
# type: boolean
# options: true | false
STREAM_INCLUDE_RAW=false

# 是否跳过动画表情图片消息（summary 为 "[动画表情]" 的纯图片）
# 跳过动画表情
# type: boolean
# options: true | false
STREAM_SKIP_ANIMATED_EMOJI=true

# 是否跳过非引用语音消息（默认 true；仅语音且无引用、无文本、无其他段时跳过，不推送到消息流）
# 跳过语音消息
# type: boolean
# options: true | false
STREAM_SKIP_VOICE=true

# 消息流 RPC 调用 NapCat 失败时是否自动重试
# 消息流 RPC 重试
# type: boolean
# options: true | false
STREAM_RPC_RETRY_ENABLED=true

# RPC 重试间隔（毫秒，默认 10000）
# RPC 重试间隔
# type: number
# range: 1000-600000
STREAM_RPC_RETRY_INTERVAL_MS=10000

# RPC 最大重试次数（默认 60 次）
# RPC 最大重试次数
# type: number
# range: 0-1000
STREAM_RPC_RETRY_MAX_ATTEMPTS=60

# 图片缓存目录（可选，默认 ./cache/images）
# 图片缓存目录
# type: string
IMAGE_CACHE_DIR=./cache/images

# 文件/音视频缓存目录（可选，默认 ./cache/file）
# 文件缓存目录
# type: string
FILE_CACHE_DIR=./cache/file

# 图片缓存过期时间（天，默认 2 天；小于等于 0 表示不自动清理）
# 图片缓存过期天数
# type: number
# range: -1-365
IMAGE_CACHE_EXPIRE_DAYS=2

# 文件缓存过期时间（天，默认 2 天；小于等于 0 表示不自动清理）
# 文件缓存过期天数
# type: number
# range: -1-365
FILE_CACHE_EXPIRE_DAYS=2