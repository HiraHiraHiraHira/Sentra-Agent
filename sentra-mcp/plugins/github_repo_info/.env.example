# 是否启用插件
# 启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# 插件超时（毫秒）
# type: number
# range: 30000-300000
PLUGIN_TIMEOUT_MS=90000

# GitHub 访问令牌（强烈建议配置以提升速率限制与权限）
# GitHub Token
# type: string
GITHUB_TOKEN=

# 等同于 GITHUB_TOKEN 的备用选项
# 备用 Token
# type: string
GH_TOKEN=

# 自定义 GitHub API 基地址
# GitHub API Base
# type: string
GITHUB_API_BASE=https://api.github.com

# GitHub API 版本
# GitHub API 版本
# type: string
GITHUB_API_VERSION=2022-11-28

# 默认返回的最大提交数量
# 最大提交数
# type: number
# range: 1-100
GITHUB_MAX_COMMITS=5

# 默认返回的最大贡献者数量
# 最大贡献者数
# type: number
# range: 1-100
GITHUB_MAX_CONTRIBUTORS=5

# 默认返回的最大标签数量
# 最大标签数
# type: number
# range: 1-100
GITHUB_MAX_TAGS=10

# 默认 README 预览长度（字符数）
# README 预览长度
# type: number
# range: 100-5000
GITHUB_README_PREVIEW_CHARS=400

# 是否默认包含仓库使用的编程语言信息
# 包含语言信息
# type: boolean
# options: true | false
GITHUB_INCLUDE_LANGUAGES=false

# 是否默认包含仓库的话题标签信息
# 包含话题标签
# type: boolean
# options: true | false
GITHUB_INCLUDE_TOPICS=false

# 是否默认包含仓库的发布版本信息
# 包含发布版本
# type: boolean
# options: true | false
GITHUB_INCLUDE_RELEASES=false

# 是否默认包含仓库的标签信息
# 包含标签
# type: boolean
# options: true | false
GITHUB_INCLUDE_TAGS=false

# 是否默认包含仓库 README 文件的预览
# 包含 README 预览
# type: boolean
# options: true | false
GITHUB_INCLUDE_README_PREVIEW=false

# 是否默认包含仓库的社区资料信息
# 包含社区资料
# type: boolean
# options: true | false
GITHUB_INCLUDE_COMMUNITY_PROFILE=false

# 是否默认包含仓库的提交活跃度统计信息
# 包含提交活跃度
# type: boolean
# options: true | false
GITHUB_INCLUDE_STATS_COMMIT_ACTIVITY=false