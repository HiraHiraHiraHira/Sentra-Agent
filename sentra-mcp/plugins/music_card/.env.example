# 是否启用插件
# 启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# 插件超时（毫秒）
# type: number
# range: 60000-300000
PLUGIN_TIMEOUT_MS=150000

# WebSocket SDK (ws_rpc) 的 URL 地址
# WS SDK 地址
# type: string
WS_SDK_URL=ws://localhost:6702

# WebSocket SDK 调用超时时间（毫秒）
# WS SDK 超时（毫秒）
# type: number
# range: 5000-60000
WS_SDK_TIMEOUT_MS=15000

# WebSocket SDK 调用参数样式
# 参数风格
# type: enum
# options: object | pair
WS_SDK_ARG_STYLE=pair

# WebSocket SDK 用于发送消息的路径（main + specific）
# 自定义发送路径
# type: string
WS_SDK_SEND_PATH=

# 私聊发送路径
# 私聊发送路径
# type: string
WS_SDK_SEND_PATH_PRIVATE=send.private

# 群聊发送路径
# 群聊发送路径
# type: string
WS_SDK_SEND_PATH_GROUP=send.group

# 网易云音乐搜索结果数量限制
# 搜索结果数量
# type: number
# range: 1-50
MUSIC163_SEARCH_LIMIT=6

# 网易云音乐 MUSIC_U Cookie（可选，用于获取更好的播放 URL）
# MUSIC_U Cookie
# type: string
MUSIC163_COOKIE_MUSIC_U=