# 前端服务端口
# 前端端口
# type: number
# range: 1024-65535
CLIENT_PORT=7244

# 后端服务端口 Fastify API 或生产环境静态文件服务端口
# 后端端口
# type: number
# range: 1024-65535
SERVER_PORT=7245


# 允许的跨域来源
# 跨域来源
# type: string
CORS_ORIGIN=*

# 登录安全令牌 - 用于身份验证的密码
# 登录令牌
# type: string
SECURITY_TOKEN=

# 日志级别 - 控制日志输出详细程度
# 日志级别
# type: enum
# options: debug | info | warn | error
LOG_LEVEL=info

# Sentra Agent 根目录 - 项目根路径
# 项目根目录
# type: string
SENTRA_ROOT=..

# Node.js 包管理器 - 用于安装 npm 依赖
# 包管理器
# type: enum
# options: npm | cnpm | pnpm | yarn | auto
PACKAGE_MANAGER=pnpm

# 依赖安装镜像配置 - 预设的镜像源方案
# 镜像源方案
# type: enum
# options: official | cn
MIRROR_PROFILE=cn

# NPM 包注册表地址 - Node.js 包下载源
# NPM 注册表
# type: string
NPM_REGISTRY=https://registry.npmmirror.com

# Python 包索引地址 - 主要的 PyPI 镜像源
# PyPI 索引
# type: string
PIP_INDEX_URL=https://pypi.tuna.tsinghua.edu.cn/simple

# Python 额外包索引 - 补充的 PyPI 镜像源
# PyPI 额外索引
# type: string
PIP_EXTRA_INDEX_URL=

# 受信任的 Python 包索引主机 - 跳过 SSL 验证的主机列表，多个主机用逗号分隔
# PyPI 受信任主机
# type: string
PIP_TRUSTED_HOSTS=pypi.tuna.tsinghua.edu.cn,pypi.org

# PyTorch 专用索引 - PyTorch 及其扩展包的下载源，example: https://download.pytorch.org/whl/cpu (CPU 版本)
# PyTorch 索引
# type: string
PYTORCH_INDEX_URL=

# 仅使用自定义索引 - 禁用官方 PyPI 回退看，设置为 true 时完全依赖自定义镜像源
# 禁用 PyPI 回退
# type: boolean
# options: true | false
PIP_ONLY_CUSTOM_INDEX=false

# 更新源类型 - 系统更新检查的远程仓库
# 更新源类型
# type: enum
# options: github | gitee | custom
UPDATE_SOURCE=gitee

# 自定义更新源地址 - 当 UPDATE_SOURCE=custom 时生效
# 自定义更新源
# type: string
UPDATE_CUSTOM_URL=https://gitee.com/yuanpluss/Sentra-Agent

# DeepWiki Chat 后端 OpenAI 兼容接口基础地址 - 可填写根地址或以 /v1 结尾（后端会自动兼容）
# DeepWiki API 地址
# type: string
DEEPWIKI_API_BASE_URL=https://yuanplus.chat

# DeepWiki Chat 使用的 API 密钥 - 不在前端暴露
# DeepWiki API 密钥
# type: string
DEEPWIKI_API_KEY=

# DeepWiki Chat 默认模型名称 - 当请求体未显式指定 model 时使用
# DeepWiki 默认模型
# type: string
DEEPWIKI_MODEL=gemini-3-flash-preview

# 终端 Shell - CMD 可执行文件绝对路径 - 配置后优先使用（兼容服务环境 PATH 不完整）CMD 路径（Windows example: C:\\Windows\\System32\\cmd.exe）
# type: string
SENTRA_SHELL_CMD_PATH=

# 终端 Shell - Windows PowerShell 可执行文件绝对路径 - 配置后优先使用（兼容服务环境 PATH 不完整）PowerShell 路径（Windows example: C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe）
# type: string
SENTRA_SHELL_POWERSHELL_PATH=

# 终端 Shell - PowerShell 7 (pwsh) 可执行文件绝对路径 - 配置后优先使用（兼容服务环境 PATH 不完整）路径（Windows example: C:\\Program Files\\PowerShell\\7\\pwsh.exe）
# type: string
SENTRA_SHELL_PWSH_PATH=

# 终端 Shell - Bash 可执行文件绝对路径 - 配置后优先使用 bash 路径（Linux example: /bin/bash；Windows Git Bash 可填写 bash.exe 的绝对路径）
# type: string
SENTRA_SHELL_BASH_PATH=

# 终端 Shell - Git Bash 可执行文件绝对路径 - 配置后优先使用（仅 Windows）Git Bash 路径（Windows example: C:\\Program Files\\Git\\bin\\bash.exe）
# type: string
SENTRA_SHELL_GIT_BASH_PATH=

# 终端 Shell - WSL 可执行文件绝对路径 - 配置后优先使用（仅 Windows）WSL 路径（Windows example: C:\\Windows\\System32\\wsl.exe）
# type: string
SENTRA_SHELL_WSL_PATH=

# 终端 Shell - Zsh 可执行文件绝对路径 - 配置后优先使用（仅 Linux/macOS）zsh 路径（Linux example: /usr/bin/zsh）
# type: string
SENTRA_SHELL_ZSH_PATH=

# 终端 Shell - sh 可执行文件绝对路径 - 配置后优先使用 sh 路径（Linux example: /bin/sh）
# type: string
SENTRA_SHELL_SH_PATH=